students = {
    "Alice": 85,
    "Bob": 92,
    "Charlie": 78,
    "Diana": 90,
    "Ethan": 88
}
student_name = input("Enter the student's name: ")
if student_name in students:
    print(f"{student_name}'s marks: {students[student_name]}")
else:
    print(f"Student '{student_name}' not found in the records.")
